{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "AgentOS",
  "colors": {
    "primary": "#EA580C",
    "light": "#F97316",
    "dark": "#C2410C"
  },
  "favicon": "/logo/agentos-transparent.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Core concepts",
            "pages": [
              "concepts/how-it-works"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Sign Up",
          "href": "https://useagentos.com",
          "icon": "rocket"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/agentos-transparent.png",
    "dark": "/logo/agentos-transparent.png",
    "text": "AgentOS"
  },
  "fonts": {
    "heading": {
      "family": "Noto Serif"
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:johann@useagentos.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://useagentos.com"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/actaso"
    }
  },
  "seo": {
    "metatags": {
      "canonical": "https://docs.useagentos.com"
    }
